<h1>Enter The Community</h1>
<%= form_for :session, :url => post_session_path do |f| %>
  <div>
    <%= f.label :email %> <br />
    <%= f.text_field :email, :placeholder => 'email', :value => @email, :required => true %> <br />
  </div>
  <p><%= @whatever %></p>
  <div>
    <%= f.label :password %> <br />
    <%= f.password_field :password, :placeholder => 'password', :required => true %> <br />
  </div>
  <div>
    <%= f.submit "Login" %> <br />
  </div>
<% end %>
