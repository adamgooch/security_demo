<h1>Enter The Community</h1>
<%= form_for(:session, :url => sessions_path) do |f| %>
  <div>
    <%= f.label :email %> <br />
    <%= f.text_field :email, :required => true %> <br />
  </div>
  <div>
    <%= f.label :password %> <br />
    <%= f.password_field :password, :required => true %> <br />
  </div>
  <div>
    <%= f.submit "Login" %> <br />
  </div>
<% end %>
